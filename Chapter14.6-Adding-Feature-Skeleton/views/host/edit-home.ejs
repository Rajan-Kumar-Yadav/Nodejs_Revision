  <body> 
    <%-include('../partials/nav') %>
    
    <main class="container mx-auto mt-8 bg-white rounded-lg shadow-md ">
      <h1 class="text-3xl font-bold mb- text-center text-green-800 mb-5"><%= editing ?"Edit": "Register" %> Your Home on Airbnb</h1>
 <form action="/host/add-home" method="POST" class="max-w-md mx-auto">
    <input type="text" name="houseName" placeholder="Enter your name" class="w-full px-4 mb-6 border rounded-lg focus:ring-red-500 border-red-500 placeholder-blue-500"  />
    <input type="text" name="price" placeholder="Price per night" class="w-full px-4 mb-6 border rounded-lg focus:ring-red-500 border-red-500 placeholder-blue-500" />
    <input type="text" name="location" placeholder="Location" class="w-full px-4 mb-6 border rounded-lg focus:ring-red-500 border-red-500 placeholder-blue-500" />
    <input type="text" name="rating" placeholder="Rating" class="w-full px-4 mb-6 border rounded-lg focus:ring-red-500 border-red-500 placeholder-blue-500" />
    <input type="text" name="photoUrl" placeholder="Enter Photo URL" class="w-full px-4 mb-6 border rounded-lg focus:ring-red-500 border-red-500 placeholder-blue-500" />
    <input type="submit" class="w-full bg-red-500 text-green-300 py-2 rounded-md hover:bg-red-600 mb-6 border-green-500 border font-bold text-3xl"/>
    </from>
    </main>
   
  </body>
</html>
